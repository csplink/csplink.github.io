{
    "site_name": "csp wiki",
    "site_slogon": "happy to write",
    "site_root_url": "/csp_wiki",
    "site_domain": "csp_wiki",
    "site_protocol": "https",
    "config_template_dir": "config",
    "source": "https://github.com/xqyjlj/csp_wiki/blob/master",
    "route": {
        "docs": {
            "/develop/zh/": "docs/develop/zh"
        },
        "pages": {
            "/": "pages/index/zh"
        },
        "assets": {
            "/static/": "static"
        },
        "blog": {
            "/blog/": "blog"
        }
    },
    "translate": {
        "docs": {
            "/develop/zh/": [ {
                    "url": "/develop/en/",
                    "src": "docs/develop/en"
                }
            ]
        },
        "pages": {
            "/": [ {
                    "url": "/en/",
                    "src": "pages/index/en"
                }
            ]
        }
    },
    "executable": {
        "python": "python3",
        "pip": "pip3"
    },
    "plugins": {
        "teedoc-plugin-markdown-parser": {
            "from": "pypi",
            "config": {
                "mathjax": {
                    "enable": true
                }
            }
        },
        "teedoc-plugin-jupyter-notebook-parser": {
            "from": "pypi",
            "config": {}
        },
        "teedoc-plugin-theme-default":{
            "from": "pypi",
            "config": {
                "dark": true,
                "env": {
                    "main_color": "#94d2ef"
                }
            }
        },
        "teedoc-plugin-search": {
            "from": "pypi",
            "config": {
                "search_hint": "Search"
            }
        },
        "teedoc-plugin-baidu-tongji": {
            "from": "pypi",
            "config": {
                "code": "4d52982572d5512e9762879ebf063c86"
            }
        },
        "teedoc-plugin-blog": {
            "from": "pypi",
            "config": {}
        },
        "teedoc-plugin-ad-hint": {
            "from": "pypi",
            "config": {
                "type": "hint",
                "label": "â˜†",
                "content": "This is a hint example(support i18n)</br><a target=\"_blank\" href=\"https://github.com/teedoc/teedoc\">Give a â˜† star </a> if you like this project ðŸ¦€ðŸ¦€",
                "show_after_s": 432000,
                "date": "2021-11-16 14:40"
            }
        },
        "teedoc-plugin-comments-gitalk": {
            "from": "pypi",
            "config": {
                "env": {
                    "clientID": "18f3d4e6ca21f088c8f1",
                    "clientSecret": "8a95a858c387c3d88c0c47c8a524519b5e78faee",
                    "repo": "csp_wiki",
                    "owner": "xqyjlj",
                    "admin": [
                        "xqyjlj"
                    ],
                    "main_color": "#94d2ef",
                    "second_color": "#0a7d43",
                    "adminAutoCreate": false
                }
            }
        },
        "teedoc-plugin-assets": {
            "from": "pypi",
            "config": {
                "header_items": [
                    "/static/css/custom.css"
                ],
                "footer_items": [
                    "/static/js/custom.js"
                ],
                "env": {
                    "main_color": "#94d2ef",
                    "second_color": "#0a7d43"
                }
            }
        }
    },
    "rebuild_changes_delay": 1,
    "robots": {
        "User-agent": "*"
    },
    "layout_root_dir": "layout"
}